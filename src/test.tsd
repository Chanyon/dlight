import {View} from "./core";



class Element extends View {
    @Prop count=0
    @PropState okk = 1

    Body() {
        div("-----")
        div(this.count)
        div(this.okk)
        button()
            .onclick(() => {
                this.okk--
            })
    }
}
export class TestTest extends View {
    @Prop jjj = 1
    @State toggle1 = 1
    @Prop hht = this.jjj + 100
    a = this.jjj + 1

    // toggle4 = this.jjj + this.toggle1 + 1
    // toggle2 = this.toggle4 + this.jjj
    // toggle3 = {ok: () => this.toggle1 + 1}
    @State apple = [1,2,3]
    Body() {
        button("+")
            .onclick(() => {
                this.toggle1 ++
                // this.jjj ++
                this.apple[0] = this.toggle1
                this.apple = [...this.apple]
            })
        div (this.hht)
        div (this.toggle1)
   
        div('--')
        For(let i of this.apple.keys()) {
            If (this.apple[i] > 1) {
                div(this.apple[i])
            }
            
        }
    }
}



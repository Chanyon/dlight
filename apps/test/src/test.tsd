import {View} from "@dlightjs/dlight";
import {_, button, div, State} from "@dlightjs/useless";

export class TestTest extends View {
    @State count = 1
    dontTransThis(count: string) {
        console.log(count)
    }
    ok = {a:100}

    Body() {
        div(ok.a)
        div(count)
        button("+")
            .onclick(() => {
                count ++
            })
        button("-")
            .onclick(() => {
                count --
            })

    }
}

import {View} from "@dlightjs/dlight";
import {_, button, div, State} from "@dlightjs/useless";

export class TestTest extends View {
    @State text = ""
    @State apples = [1,2,3]
    @State orange = [1,2,3]
    @State toggle = true
    @State hh = 1
    @State fine = 2
    asts = []

    @View
    ok({a}): any {
        div(a)
    }

    Body() {
        div("hh")
        button("a")
            .onclick(() => {
                this.hh ++
                this.fine ++
            })
        this.ok()
            .a(`${this.hh} ${this.fine}`)
    }
}

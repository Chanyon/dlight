import {View, loopNodes, DLNode, DLNodeType, toEls} from "@dlightjs/dlight"



export class Transition extends View {
    @Prop _$content = 0.5

    Body() { 
        {{this._$children}}
            .deepLoopEl(true)
            ._transition((typeof this._$content === "string") ? this._$content : `all ${this._$content}s`)
    }
}

